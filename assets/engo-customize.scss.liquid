        height: 100px;
        img {
          object-fit: contain;
        }
      }
      @media (max-width: 767px) {
        img {
          object-fit: contain;
        }
      }
    }
    .slick-product-prev {
      position: absolute;
      top: calc(50% - 8px);
      left: 7px;
      font-size: 16px;
      transition: .3s;
      outline:none;
    }
    .slick-product-next {
      position: absolute;
      top: calc(50% - 8px);
      right: 7px;
      font-size: 16px;
      transition: .3s;
      outline:none;
    }
    .slick-product-prev, .slick-product-next {
