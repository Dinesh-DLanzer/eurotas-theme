@keyframes myAnimation {
  0% {transform: scale(1); opacity: 1;}
  100% {transform: scale(1.5);opacity: 0;}
}

@keyframes myAnimation1 {
  0% {transform: scale(0);opacity:1}
  50% {transform: scale(1.4)}
  100% {transform: scale(1.4);opacity: 0}
}

@keyframes myAnimation2 {
  0% {transform: rotate(0) scale(1); opacity: 0.5;}
  20% {transform: rotate(30deg) scale(1.2); opacity: 0.6;}
  40% {transform: rotate(-30deg) scale(1.3); opacity: 0.7;}
  60% {transform: rotate(30deg) scale(1.4); opacity: 0.8;}
  80% {transform: rotate(-30deg) scale(1.5);opacity: 0.9;}
}

@keyframes myAnimation3 {
  from {transform:translateY(-100%);}
  to {transform:translateY(0);}
}
@-moz-keyframes myAnimation3 {
  from {transform:translateY(-100%);}
  to {transform:translateY(0);}
}
@-webkit-keyframes myAnimation3 {
  from {transform:translateY(-100%);}
  to {transform:translateY(0);}
}
@-o-keyframes myAnimation3 {
  from {transform:translateY(-100%);}
  to {transform:translateY(0);}
}

@keyframes aniName {
  0% {
    transform: translate(0);
  }

  25% {
    transform: translate(2%);
  }
  50% {
    transform: translate(0);
  }
  75% {
    transform: translate(-2%);
  }

  100% {
    transform: translate(0);
  }
}

@-moz-keyframes aniName {
  0% {
    left: 0px;
  }

  50% {
    left: 2%;
  }

  100% {
    left: 0px;
  }
}

@-webkit-keyframes aniName {
  0% {
    left: 0px;
  }

  50% {
    left: 2%;
  }

  100% {
    left: 0px;
  }
}

@-o-keyframes aniName {
  0% {
    left: 0px;
  }

  50% {
    left: 2%;
  }

  100% {
    left: 0px;
  }
}

@-webkit-keyframes shine {
  100% {
    left: 125%
  }
}
@keyframes shine {
  100% {
    left: 125%
  }
