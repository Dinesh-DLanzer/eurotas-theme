  .countdown-image {
    position: relative;
    text-align: center;
    .lookbook-point {
      position: absolute;
      color: #fff;
      text-align: center;
      border-radius: 50%;
      width: 35px;
      height: 35px;
      box-shadow: 0 0 3px #00000010;
      transition: .3s;
      line-height: 40px;
      @media (max-width: 767px) {
        width: 25px;
        height: 25px;
      }
      span {
        &:before, &:after {
          content: "";
          width: 18px;
          height: 4px;
          background: white;
          position: absolute;
          left: 9px;
          top: 15px;
          @media (max-width: 767px) {
            left: 6px;
            width: 15px;
            top: 12px;
          }
        }
        &:before {
          transform: rotate(90deg);
        }
      }
      &:before {
        content: "";
        position: absolute;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        z-index: 0;
        border-radius: 50%;
        background: #c50215;
        animation: myAnimation1 1.5s infinite;
        -webkit-animation: myAnimation1 1.5s infinite;
        -o-animation: myAnimation1 1.5s infinite;
        -moz-animation: myAnimation1 1.5s infinite;
      }
      .popup-lookbook {
        position: absolute;
        opacity: 0;
        visibility: hidden;
        transform: translateY(10px);
        transition: .3s;
        width: 280px;
        left: -28px;
        bottom: 65px;
        background: #c50215;
        color: #fff;
        padding: 10px 20px;
        border-radius: 10px;
        &:before {
          content: '';
          display: block;
          position: absolute;
          bottom: -12px;
          left: 40px;
          border-top: 12px solid $color-hover;
          border-left: 6px solid transparent;
          border-right: 6px solid transparent;
        }
        h5 {
          font-size: 20px;
          font-weight: 700;
        }
        p {
          font-family: $font-primary;
          font-size: 18px;
          font-weight: 400;
          line-height: 23px;
          text-align: left;
          margin-bottom: 0;
        }
      }
      &:hover, &:focus {
        .popup-lookbook {
          opacity: 1;
          visibility: visible;
          transform: none;
        }
      }
    }
    @media (max-width: 991px) {
      margin-bottom: 50px;
    }
    @media (max-width: 767px) {
      margin-bottom: 20px;
    }
  }
  .content-popup-lookbook {
    left: 0;
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    display: flex;
    text-align: center;
    z-index: 102;
    opacity: 0;
    visibility: hidden;
    transition: .3s;
    overflow: scroll;
    &.active {
      opacity: 1;
      visibility: visible;
      .popup-lookbook {
        transform: none;
      }
    }
  }
  .popup-lookbook {
    transition: .3s;
    max-width: calc(100% - 30px);
    @media (min-width: 1025px) {
      max-width: 800px;
    }
    background: #fff;
    padding: 20px;
    margin: auto;
    text-align: left;
    z-index: 102;
    position: relative;
    transform: scale(.5);
    transition: .3s;
    @media (max-width: 1024px) {
      height: 60%;
      overflow: hidden;
      overflow: scroll;
    }
    @media (max-width: 576px) {
      height: 80%;
      overflow: hidden scroll;
    }
    .lookbook-content {
      h5 {
        font-size: 20px;
        font-weight: 700;
        color: $main-color;
      }
      p {
        font-family: $font-primary;
        font-size: 18px;
        font-weight: 400;
        line-height: 23px;
        text-align: left;
        margin-bottom: 0;
        color: $main-color;
      }
    }

    .slick-dots {
      display: table!important;
      margin: 0 auto;
      position: relative;
      z-index: 1;
      line-height: 100%;
      bottom: 0;
      li {
        display: inline-block;
        line-height: 100%;
        width: 5px;
        button {
          background-color: $color-hover;
          font-size: 0;
          width: 5px;
          height: 5px;
          padding: 0;
          margin: 0 4px;
          opacity: .3;
          transition: all .5s ease-out;
          &:before {
            opacity: 0;
          }
        }

        &.slick-active {
          width: 24px;
          button {
            width: 24px;
            opacity: 1;
            margin-right: 10px;
          }
        }
      }
    }
  }

  .close-popup-lookbook {
    display: inline-block;
    position: absolute;
    width: 46px;
    height: 46px;
    top: -23px;
    right: -23px;
    z-index: 999;
    border-radius: 50%;
    text-align: center;
    background-color: $color-hover;
    color: #fff;
    transition: .3s;
    @media (max-width: 1024px) {
      top: calc(20% - 23px);
      right: 0;
    }
    @media (max-width: 576px) {
      top: calc(10% - 23px);
      right: 0;
    }
    &:hover,&:focus {
      transform: rotate(180deg);
    }
    i {
      position: static;
    }
    i:before {
      font-size: 24px;
      font-weight: 300;
      line-height: 46px;
    }
    i:not([class]) {
      &:before, &:after {
        content: "";
        width: 20px;
        height: 1px;
        background: white;
        position: absolute;
        left: 13px;
        top: 22px;
      }
      &:before {
        transform: rotate(45deg);
      }
      &:after {
        transform: rotate(-45deg);
      }
    }
  }
  .bg-overlay-popup-lookbook {
    height: 100%;
    position: absolute;
    width: 100%;
    background: rgba(0, 0, 0, 0.6);
    opacity: 0;
    visibility: hidden;
    transition: .3s;
    &.active {
      opacity: 1;
      visibility: visible;
    }
  }
}

