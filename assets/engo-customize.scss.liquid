/*********************************
BACK TO TOP
*********************************/

#back-to-top {
  display: inline-block;
  background-color: $color-hover;
  width: 50px;
  height: 50px;
  text-align: center;
  border-radius: 50%;
  position: fixed;
  bottom: 60px;
  right: 30px;
  transition: background-color .3s, 
    opacity .5s, visibility .5s;
  opacity: 0;
  visibility: hidden;
  z-index: 10;
  cursor: pointer;
  i {
    line-height: 50px;
    font-size: 20px;
    color: #fff;
    &:after {
      position: absolute;
      content: "";
      background-color: transparent;
      width: 50px;
      top: 0;
      left: 0;
      height: 50px;
      box-shadow: 0 0 4px $main-color;
      outline: none;
      border-radius: 50%;
      animation: myAnimation 1.5s infinite;
      -webkit-animation: myAnimation 1.5s infinite;
      -o-animation: myAnimation 1.5s infinite;
      -moz-animation: myAnimation 1.5s infinite;
    }
  }
}

#back-to-top.show {
  opacity: 1;
  visibility: visible;
  cursor: pointer;
}

