  }.content-popup-lookbook {
    left: 0;
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    display: flex;
    text-align: center;
    z-index: 102;
    opacity: 0;
    visibility: hidden;
    transition: .3s;
    overflow: scroll;
    &.active {
      opacity: 1;
      visibility: visible;
      .popup-lookbook {
        transform: none;
      }
    }
  }
  .popup-lookbook {
    transition: .3s;
    max-width: calc(100% - 30px);
